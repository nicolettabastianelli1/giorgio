{% macro content(obj) %}
<div class="txtImage">
    <ul class="txtImage__list">
        <li class="txtImage__item">{{obj.progetto}}</li>
        <li class="txtImage__item">{{obj.titolo}}</li>
        <li class="txtImage__item">{{obj.anno}}</li>
    </ul>
    <img alt="{{obj.alt}}" class="txtImage__img" src="{{obj.src}}"></img>
</div>
{% endmacro %}
