{% macro content(obj) %}
<div class="skills">
    <div class="skills__left">Skill</div>
    <div class="skills__right">
        <ul class="skills__list">
          {% for skillItem in obj.skillList %}
            <li class="skills__item">{{skillItem.text}}</li>
        {%endfor%}
        </ul>
    </div>

</div>
{% endmacro %}
